[ignore]
.*/.yarn-cache/.*
.*/config-chain/.*
.*/node_modules

[include]
src/**/*.js

[libs]
flow-typed

[lints]
all=warn
sketchy-null=off
untyped-import=off

[options]
include_warnings=true
module.ignore_non_literal_requires=true
traces=1
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
