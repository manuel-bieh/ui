{
  "name": "@manuel-bieh/ui",
  "version": "2.3.7",
  "author": "Manuel Bieh <info@manuelbieh.de> (https://github.com/manuelbieh)",
  "homepage": "https://www.github.com/manuel-bieh/ui",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "publishConfig": {
    "tag": "beta"
  },
  "engines": {
    "node": "<=16",
    "yarn": "^1.10.0"
  },
  "scripts": {
    "analyze": "yarn build:withStats && yarn analyze:bundle",
    "analyze:bundle": "webpack-bundle-analyzer bundle-stats.json -p 8889",
    "babel:build": "babel -d dist/es ./src --extensions .ts,.tsx,.js,.jsx --no-comments src --minified --ignore **/*.d.ts",
    "babel:watch": "babel -d dist/es ./src --extensions .ts,.tsx,.js,.jsx --ignore **/*.d.ts --watch",
    "build": "cross-env NODE_ENV=production yarn clean && yarn imagemin && concurrently \"yarn copy\" \"yarn build:es\" \"yarn build:bundle\" \"yarn build:types\"",
    "build:es": "yarn transpile",
    "build:bundle": "cross-env NODE_ENV=production node scripts/build.js",
    "build:types": "tsc --outDir dist/es --emitDeclarationOnly --noEmit false --declaration --allowJs false --checkJs false",
    "build:withStats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > bundle-stats.json",
    "build:watch": "cross-env NODE_ENV=development node scripts/build.js --watch",
    "clean": "rimraf ./dist",
    "copy": "yarn copy:fonts",
    "copy:fonts": "cpy src/assets/fonts/* dist/es/assets/fonts",
    "imagemin": "cross-env imagemin-power '**/*' -r --cwd src/assets --out-dir dist/es/assets",
    "lint": "concurrently \"yarn-or-npm lint:js\" \"yarn-or-npm lint:css\"",
    "lint:js": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:css": "stylelint src/**/*.css",
    "release": "semantic-release",
    "postrelease": "git push origin master --follow-tags",
    "postcss:build": "postcss src/{**/,}*.css --base src -d dist/es",
    "postcss:watch": "postcss src/{**/,}*.css --base src -d dist/es --watch",
    "report:todos": "leasot src/{**/,}*.{js,jsx,ts,tsx}",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider yarn storybook",
    "storybook": "sb dev",
    "build-storybook": "sb build",
    "storybook5": "start-storybook -p 6006",
    "storybook5:build": "build-storybook",
    "storybook:deploy": "surge --project ./storybook-static --domain manuelbieh-ui.surge.sh",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "yarn test --updateSnapshot",
    "tsc": "tsc --noEmit",
    "transpile": "concurrently \"yarn babel:build\" \"yarn postcss:build\"",
    "watch": "concurrently \"yarn babel:watch\" \"yarn postcss:watch\""
  },
  "moduleRoots": [
    "src",
    "node_modules"
  ],
  "files": [
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "classnames": "^2.5.1",
    "core-js": "^3.35.1",
    "immer": "^10.0.3",
    "plop": "^4.0.1",
    "react": "^18.2.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0"
  },
