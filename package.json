{
  "name": "@manuel-bieh/ui",
  "version": "2.2.1",
  "author": "Manuel Bieh <info@manuelbieh.de> (https://github.com/manuelbieh)",
  "homepage": "https://www.github.com/manuel-bieh/ui",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "publishConfig": {
    "tag": "beta"
  },
  "engines": {
    "node": ">=8.12.0",
    "yarn": "^1.10.0"
  },
  "scripts": {
    "analyze": "yarn build:withStats && yarn analyze:bundle",
    "analyze:bundle": "webpack-bundle-analyzer bundle-stats.json -p 8889",
    "babel:build": "babel -d dist/es ./src --extensions .ts,.tsx,.js,.jsx --no-comments src --minified --ignore **/*.d.ts",
    "babel:watch": "babel -d dist/es ./src --extensions .ts,.tsx,.js,.jsx --ignore **/*.d.ts --watch",
    "build": "cross-env NODE_ENV=production yarn clean && yarn imagemin && concurrently \"yarn copy\" \"yarn build:es\" \"yarn build:bundle\" \"yarn build:types\"",
    "build:es": "yarn transpile",
    "build:bundle": "cross-env NODE_ENV=production node scripts/build.js",
    "build:types": "tsc --outDir dist/es --emitDeclarationOnly --noEmit false --declaration --allowJs false --checkJs false",
    "build:withStats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > bundle-stats.json",
    "build:watch": "cross-env NODE_ENV=development node scripts/build.js --watch",
    "clean": "rimraf ./dist",
    "copy": "yarn copy:fonts",
    "copy:fonts": "cpy src/assets/fonts dist/es/assets/fonts",
    "imagemin": "cross-env imagemin-power '**/*' -r --cwd src/assets --out-dir dist/es/assets",
    "lint": "concurrently \"yarn-or-npm lint:js\" \"yarn-or-npm lint:css\"",
    "lint:js": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:css": "stylelint src/**/*.css",
    "release": "semantic-release",
    "postrelease": "git push origin master --follow-tags",
    "postcss:build": "postcss src/{**/,}*.css --base src -d dist/es",
    "postcss:watch": "postcss src/{**/,}*.css --base src -d dist/es --watch",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:deploy": "surge --project ./storybook-static --domain manuelbieh-ui.surge.sh",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "yarn test --updateSnapshot",
    "tsc": "tsc --noEmit",
    "transpile": "concurrently \"yarn babel:build\" \"yarn postcss:build\"",
    "watch": "concurrently \"yarn babel:watch\" \"yarn postcss:watch\""
  },
  "moduleRoots": [
    "src",
    "node_modules"
  ],
  "files": [
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "@storybook/addon-storysource": "^5.1.9",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "immer": "^3.1.3",
    "react": "^16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@semantic-release/git": "^7.0.16",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@testing-library/react": "^8.0.5",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.1",
    "concurrently": "^4.1.1",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.7",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-wiremore": "^3.0.4",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^9.1.1",
    "file-loader": "^4.0.0",
    "github-markdown-css": "^3.0.1",
    "history": "^4.9.0",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^5.0.0",
    "imagemin-cli": "^5.0.0",
    "imagemin-power-cli": "^3.0.0",
    "install-deps-postmerge": "^1.0.4",
    "jest": "^24.8.0",
    "jest-haste-map": "^24.8.1",
    "jest-resolve": "^24.8.0",
    "lint-staged": "^9.2.0",
    "marked": "^0.7.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss": "^7.0.17",
    "postcss-assets": "^5.0.0",
    "postcss-cli": "^6.1.3",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.1",
    "postcss-discard-comments": "^4.0.1",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-discard-empty": "^4.0.1",
    "postcss-each": "^0.10.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-normalize": "^8.0.1",
    "prettier": "^1.18.2",
    "raw-loader": "^3.0.0",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.13.18",
    "semantic-release-conventional-commits": "^2.0.1",
    "stats-webpack-plugin": "^0.7.0",
    "storybook-addon-jsx": "^7.1.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-order": "^3.0.1",
    "stylelint-prettier": "^1.1.1",
    "surge": "^0.21.3",
    "typescript": "^3.5.3",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.4.1"
  },
  "peerDependencies": {
    "classnames": "^2.2.6"
  },
  "husky": {
    "hooks": {
      "post-merge": "install-deps-postmerge",
      "pre-commit": "lint-staged"
    }
  }
}
