{
  "name": "@manuel-bieh/ui",
  "version": "1.1.10",
  "author": "Manuel Bieh <info@manuelbieh.de> (https://github.com/manuelbieh)",
  "license": "MIT",
  "main": "dist/main.js",
  "module": "es/index.js",
  "engines": {
    "node": ">=8.12.0",
    "yarn": "^1.10.0"
  },
  "scripts": {
    "analyze": "yarn build:withStats && yarn analyze:bundle",
    "analyze:bundle": "webpack-bundle-analyzer bundle-stats.json -p 8889",
    "babel:build": "babel -d es ./src",
    "babel:watch": "babel -d es ./src --watch",
    "build": "yarn imagemin && yarn build:es && yarn build:compiled",
    "build:es": "yarn transpile",
    "build:compiled": "cross-env NODE_ENV=production node scripts/build.js",
    "build:withStats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > bundle-stats.json",
    "build:watch": "cross-env NODE_ENV=development node scripts/build.js --watch",
    "clean:es": "rimraf ./es",
    "flow": "flow",
    "imagemin:clients": "imagemin src/assets/img/clients/* --out-dir=assets/img/clients",
    "imagemin:social-media": "imagemin src/assets/img/social-media/* --out-dir=assets/img/social-media",
    "imagemin:tech": "imagemin src/assets/img/tech/* --out-dir=assets/img/tech",
    "imagemin": "concurrently \"yarn imagemin:clients\" \"yarn imagemin:social-media\" \"yarn imagemin:tech\"",
    "lint": "eslint ./src",
    "postmerge": "install-deps-postmerge",
    "precommit": "lint-staged",
    "release": "semantic-release --no-ci",
    "postversion": "yarn build",
    "postrelease": "git push origin master --follow-tags",
    "postcss:build": "postcss src/{**/,}*.css --base src -d es",
    "postcss:watch": "postcss src/{**/,}*.css --base src -d es --watch",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:deploy": "surge --project ./storybook-static --domain manuelbieh-ui.surge.sh",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "yarn test --updateSnapshot",
    "transpile": "yarn clean:es && yarn babel:build && yarn postcss:build",
    "watch": "concurrently \"yarn babel:watch\" \"yarn postcss:watch\""
  },
  "lint-staged": {
    "*.{js,css}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint src",
      "git add"
    ]
  },
  "moduleRoots": [
    "src",
    "node_modules"
  ],
  "files": [
    "assets/*",
    "lib/*",
    "dist/*",
    "src/*",
    "es/*"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.6.1",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.1",
    "react-helmet": "^5.2.0",
    "react-i18next": "^8.3.7",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-backgrounds": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-links": "^4.0.4",
    "@storybook/addon-options": "^4.0.4",
    "@storybook/addon-storysource": "^4.0.4",
    "@storybook/addons": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "cssnano": "^4.1.7",
    "imagemin-cli": "^3.0.0",
    "install-deps-postmerge": "^1.0.2",
    "postcss-custom-media": "^6.0.0",
    "postcss-discard-comments": "^4.0.1",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-discard-empty": "^4.0.1",
    "postcss-each": "^0.10.0",
    "raw-loader": "^0.5.1",
    "storybook-addon-jsx": "^5.4.0",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@semantic-release/git": "^7.0.5",
    "@storybook/addon-info": "^4.0.6",
    "autoprefixer": "^9.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-wiremore": "^2.2.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^0.0.0",
    "eslint-plugin-security": "^1.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.79.1",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.5.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "mini-css-extract-plugin": "^0.4.4",
    "postcss": "^7.0.5",
    "postcss-assets": "^5.0.0",
    "postcss-cli": "^6.0.1",
    "postcss-custom-properties": "^7.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-normalize": "^6.0.0",
    "prettier": "^1.15.2",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.10.8",
    "semantic-release-conventional-commits": "^2.0.1",
    "stats-webpack-plugin": "^0.6.2",
    "surge": "^0.20.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.4.1"
  },
  "peerDependencies": {
    "classnames": "^2.2.6"
  }
}
